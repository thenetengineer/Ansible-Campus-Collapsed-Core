                    {% endif %}
                    {# --------------------------- #}
                    {# from.is-fragment            #}
                    {# --------------------------- #}
                    {% if item.from.is_fragment is defined %}
                    is-fragment;
                    {% endif %}
                    {# --------------------------- #}
