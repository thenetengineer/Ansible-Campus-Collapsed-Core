        {# ----------------------------------------------------------------------------------- #}
        {% if configuration.firewall.family.ethernet_switching.filter is not mapping and configuration.firewall.family.ethernet_switching.filter is not string %}
        {% for each in configuration.firewall.family.ethernet_switching.filter %}
        filter {{ each.name }} {
            {# ----------------------------------------------------------------------------------- #}
            {# multiple terms                                                                      #}
            {# ----------------------------------------------------------------------------------- #}
            {% if each.term is not mapping and each.term is not string %}
            {% for item in each.term %}
            term {{ item.name }} {
                from {
                    {% if item.from.ether_type is defined %}
                    ether-type [ {{ item.from.ether_type|join(' ') }} ];
                    {% elif item.from.ip_protocol %}
                    ip-protocol [ {{ item.from.ip_protocol|join(' ') }} ];
                    {% endif %}
                }
                {% if item.then.discard is defined %}
                then discard;
                {% elif item.then.accept is defined %}
                then accept;
                {% endif %}
            }
            {% endfor %}
            {# ----------------------------------------------------------------------------------- #}
            {# single term                                                                         #}
            {# ----------------------------------------------------------------------------------- #}
            {% else %}
